  3.757019561982154*^9}},ExpressionUUID->"8fb3510b-8f60-427d-9ccc-\
59c60f137b90"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{
     FractionBox["1", 
      RowBox[{"2", 
       SubscriptBox["\[Mu]", 
        SubscriptBox["R", "1"]]}]], 
     SuperscriptBox[
      SubscriptBox["p", 
       SubscriptBox["R", "1"]], "2"], 
     RowBox[{"\[Psi]", "(", 
      OverscriptBox["r", 
       StyleBox["\[RightVector]",
        FontSize->18]], ")"}]}], "+", 
    RowBox[{
     FractionBox["1", 
      RowBox[{"2", 
       SubscriptBox["\[Mu]", 
        SubscriptBox["R", "2"]]}]], 
     SuperscriptBox[
      SubscriptBox["p", 
       SubscriptBox["R", "2"]], "2"], 
     RowBox[{"\[Psi]", "(", 
      OverscriptBox["r", 
       StyleBox["\[RightVector]",
        FontSize->18]], ")"}]}]}], " ", "=", " ", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       FractionBox[
        SuperscriptBox[
         SubscriptBox["Q", "\[Alpha]\[Alpha]"], "2"], 
        RowBox[{"2", 
         SubscriptBox["\[Mu]", 
          SubscriptBox["R", "1"]]}]], "+", 
       FractionBox[
        SuperscriptBox[
         SubscriptBox["Q", "\[Alpha]\[Beta]"], "2"], 
        RowBox[{"2", 
         SubscriptBox["\[Mu]", 
          SubscriptBox["R", "2"]]}]]}], ")"}], 
     RowBox[{
      FractionBox[
       SuperscriptBox["\[PartialD]", "2"], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["\[Alpha]", "2"]}]], 
      RowBox[{"\[Psi]", "(", 
       OverscriptBox["r", 
        StyleBox["\[RightVector]",
         FontSize->18]], ")"}]}]}], "+", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       FractionBox[
        SuperscriptBox[
         SubscriptBox["Q", "\[Alpha]\[Beta]"], "2"], 
        RowBox[{"2", 
         SubscriptBox["\[Mu]", 
          SubscriptBox["R", "1"]]}]], "+", 
       FractionBox[
        SuperscriptBox[
         SubscriptBox["Q", "\[Beta]\[Beta]"], "2"], 
        RowBox[{"2", 
         SubscriptBox["\[Mu]", 
          SubscriptBox["R", "2"]]}]]}], ")"}], 
     RowBox[{
      FractionBox[
       SuperscriptBox["\[PartialD]", "2"], 
       RowBox[{"\[PartialD]", 
        SuperscriptBox["\[Beta]", "2"]}]], 
      RowBox[{"\[Psi]", "(", 
       OverscriptBox["r", 
        StyleBox["\[RightVector]",
         FontSize->18]], ")"}]}]}]}]}], 
  TraditionalForm]], "ExpositoryMathCentered",
 CellChangeTimes->{{3.757019596606657*^9, 3.7570196417337914`*^9}, {
  3.757019677590075*^9, 3.7570197585109463`*^9}, {3.757019978073773*^9, 
  3.757019978431419*^9}},ExpressionUUID->"d5cf83be-1462-4221-9b5c-\
4a7e673b8819"],

Cell[TextData[{
 "Then making use of the fact that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     SubscriptBox["Q", 
      RowBox[{"\[InvisibleSpace]", 
       RowBox[{"i", "\[InvisibleSpace]", "j"}]}]], "2"], "=", 
    RowBox[{
     SuperscriptBox[
      SubscriptBox["Q", 
       RowBox[{"n", "\[InvisibleSpace]", "m"}]], "2"], "=", 
